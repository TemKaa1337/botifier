{
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "DependencyInjection",
            "Model/Request",
            "Model/Response",
            "Model/Shared",
            "Factory/Response",
            "Factory/Shared",
        ]
    },
    "timeout": 10,
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            "usleep.*"
        ]
    },
    "minMsi": 100
}
